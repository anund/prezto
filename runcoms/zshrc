# Source Prezto.
if [[ -s "${ZDOTDIR:-$HOME}/.zprezto/init.zsh" ]]; then
  source "${ZDOTDIR:-$HOME}/.zprezto/init.zsh"
fi

# Customize to your needs...

#vim
bindkey -M viins 'jj' vi-cmd-mode

alias vi=vim

# exports
export SSH_ASKPASS=/usr/lib/ssh/x11-ssh-askpass

